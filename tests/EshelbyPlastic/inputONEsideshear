alamo.program               = crystalPlastic
plot_file		    = tests/EshelbyPlastic/output

# this is not a time integration, so do
# exactly one timestep and then quit
timestep		    = 0.00001
stop_time		    = 0.00001
# amr parameters
amr.plot_int		    = 1
amr.max_level		    = 0
amr.max_grid_size(1)	    = 1000000000000
amr.n_cell		    = 32 32 32
amr.blocking_factor	    = 4
amr.regrid_int		    = 1
amr.grid_eff		    = 1.0

# geometry
geometry.prob_lo	    = -8 -8 -8 
geometry.prob_hi	    = 8 8 8
geometry.is_periodic	    = 0 0 0

# ellipse configuration
ic.type        = ellipse
ic.ellipse.a   = 1.0 0.75 0.5  # ellipse radii
ic.ellipse.x0  = 0 0 0 # location of ellipse center
ic.ellipse.eps = 0.1 # diffuse boundary
ic.Finclusion  = 0.001 0 0 0 0.001 0 0 0 0.001 # eigenstrain

# elastic moduli
# Normalized values with Tcrss
# c11 = 10e3 c12 = 7.35e3 c44 = 3.8e3 Tcrss = 2.75e-3
elastic.model.c11 = 27.5 
elastic.model.c12 = 20.2125
elastic.model.c44 = 10.45

elastic.solver.verbose = 2
elastic.solver.nriters = 1

# displacement types
elastic.bc.type.ylo = trac trac trac
elastic.bc.type.yhi = trac trac trac
elastic.bc.type.xlo = disp disp disp
elastic.bc.type.xhi = disp disp disp
elastic.bc.type.zlo = trac trac trac
elastic.bc.type.zhi = trac trac trac
# displacement values
elastic.bc.val.xlo = 0 0 0 
elastic.bc.val.xhi = 0 0.01 0 
 # vertices 
elastic.bc.type.xhiylozlo = disp disp disp
elastic.bc.type.xhiyhizlo = disp disp disp
elastic.bc.type.xhiyhizhi = disp disp disp
elastic.bc.type.xhiylozhi = disp disp disp

elastic.bc.val.xhiylozlo = 0 0.01 0 
elastic.bc.val.xhiyhizlo = 0 0.01 0 
elastic.bc.val.xhiyhizhi = 0 0.01 0 
elastic.bc.val.xhiylozhi = 0 0.01 0 

elastic.bc.val.xloylozlo = 0 0 0 
elastic.bc.val.xloyhizlo = 0 0 0 
elastic.bc.val.xloyhizhi = 0 0 0 
elastic.bc.val.xloylozhi = 0 0 0 
# edge type

elastic.bc.type.zloxhi = disp disp disp
elastic.bc.type.zloxlo = disp disp disp
elastic.bc.type.zhixhi = disp disp disp
elastic.bc.type.zhixlo = disp disp disp

elastic.bc.type.xhiylo = disp disp disp
elastic.bc.type.xhiyhi = disp disp disp
elastic.bc.type.xloylo = disp disp disp
elastic.bc.type.xloyhi = disp disp disp

elastic.bc.type.yhizhi = trac trac trac
elastic.bc.type.ylozhi = trac trac trac
elastic.bc.type.yhizlo = trac trac trac
elastic.bc.type.ylozlo = trac trac trac

#edge values

elastic.bc.val.zloxhi = 0 0.01 0 
elastic.bc.val.zloxlo = 0 0 0 
elastic.bc.val.zhixhi = 0 0.01 0 
elastic.bc.val.zhixlo = 0 0 0 

elastic.bc.val.xhiylo = 0 0.01 0 
elastic.bc.val.xhiyhi = 0 0.01 0 
elastic.bc.val.xloylo = 0 0 0 
elastic.bc.val.xloyhi = 0 0 0 

