alamo.program               = eshelby
plot_file		    = tests/Eshelby/output

# this is not a time integration, so do
# exactly one timestep and then quit
timestep		    = 0.1
stop_time		    = 0.1

# amr parameters
amr.plot_int		    = 1
amr.max_level		    = 5
amr.n_cell		    = 32 32 32
amr.blocking_factor	    = 4
amr.regrid_int		    = 1
amr.grid_eff		    = 1.0

# geometry
geometry.prob_lo	    = -8 -8 -8 
geometry.prob_hi	    = 8 8 8
geometry.is_periodic	    = 0 0 0

# ellipse configuration
ic.type        = ellipse
ic.a           = 1.0 0.75 0.5  # ellipse radii
ic.x0          = 0 0 0 # location of ellipse center
ic.eps         = 0.1 # diffuse boundary
ic.Finclusion  = 0.001 0 0 0 0.001 0 0 0 0.001 # eigenstrain

# elastic moduli
elastic.E = 210 
elastic.nu = 0.3

