geometry.prob_hi = 5.0  2.0 0.0 # [ m ]

geometry.is_periodic = 0 0 0

### TIME STEP ###

timestep  = 1.0e-4
stop_time = 50.0

### ETA IC ###

rhoInterface.ic.type = expression
rhoInterface.ic.expression.region0 = "500.0"

vInterface.ic.type = expression
