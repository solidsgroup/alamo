alamo.program = microstructure

plot_file = tests/ManualDisconnections/output/testNucleationTypes/fixed_down_noncenter

timestep = 0.05
stop_time=80.0
amr.plot_dt=0.1
amr.max_level=3
amr.n_cell=64 64 64
amr.blocking_factor=8
amr.regrid_int=10
amr.grid_eff=1.0
amr.max_grid_size=8
amr.thermo.plot_dt=0.1

geometry.prob_lo=0 -4 0
geometry.prob_hi=8 4 8
geometry.is_periodic=0 0 0

bc.eta.type.xlo=neumann
bc.eta.type.xhi=neumann
bc.eta.type.ylo=EXT_DIR
bc.eta.type.yhi=EXT_DIR
bc.eta.val.ylo=1.0 0.0
bc.eta.val.yhi=0.0 1.0

pf.number_of_grains=2
pf.M=1.0
pf.gamma=1.0
pf.l_gb=0.04
pf.sigma0=0.075
pf.elastic_mult=200
pf.elastic_threshold=0.004

anisotropy.on=0
anisotropy.gb_type = read
anisotropy.filename = tests/ManualDisconnections/gbenergy_cu_001sigma13_smooth.dat
anisotropy.timestep=0.0005
anisotropy.elastic_int = 100
anisotropy.thermo_int = 10
anisotropy.tstart= 1.0
anisotropy.damp=1.0

fluctuation.on=0
fluctuation.amp=0.001
fluctuation.sd=1.0
fluctuation.tstart=1.0

disconnection.on=1
disconnection.tstart=2.1
disconnection.nucleation_energy=1.3806503e-23
disconnection.temp= 0.125    # probability 1/e^8
disconnection.box_size=0.5

disconnection.fixed=1
disconnection.fixed_site=2.0
disconnection.fixed_phase=1
disconnection.interval=4


elastic.on = 0
elastic.tstart = 2.0
elastic.interval = 1
elastic.solver.verbose = 2

# Copper (Bercegeau & Bernard, PRB) - Sigma13
elastic.model1.C11 = 1.71
elastic.model1.C12 = 1.22
elastic.model1.C44 = 0.75
elastic.model2.Phi = 0.19739674
elastic.model1.F0 = 0 0.2 0 0

# Sigma13
elastic.model2.C11 = 1.71
elastic.model2.C12 = 1.22
elastic.model2.C44 = 0.85
elastic.model2.Phi = -0.19739674
elastic.model2.F0 = 0 -0.2 0 0

# Elastic BCs
elastic.bc.type.xlo = neumann neumann
elastic.bc.type.xhi = neumann neumann

anisotropy.beta= 0.00005
ic.type=tabulated_interface
ic.xs = 0.0 8.0
ic.ys = 0.0 0.0
elastic.bc.val.xhiyhi  = (2,60.0:0.2,0.3) 0
elastic.bc.val.xloyhi  = (2,60.0:0.2,0.3) 0
elastic.bc.val.yhi     = (2,60.0:0.2,0.3) 0
